{
  "name": "translate-go",
  "version": "1.0.5",
  "description": "前端翻譯工具",
  "main": "./dist/translate-go/lib.js",
  "typings": "./dist/translate-go/lib.d.ts",
  "author": "clarewu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/babyblue94520/translate-go.git"
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run toolbar && npm run go",
    "rollupbuild": "rollup -c rollup.config.js --no-conflict",
    "rollupbuild:min": "rollup -c rollup.config.min.js",
    "start": "ng serve --port 4300",
    "go": "tsc -p src/translate-go/tsconfig.go.json",
    "toolbar": "ng build --configuration production --base-href /translate-go/dist/ && gulp",
    "http": "ng build --configuration production --output-hashing=none && gulp && http-server --gzip",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update": "ng update @angular/cli@8 @angular/core@8 --allow-dirty --force & ng update @angular/cli@9 @angular/core@9 --allow-dirty --force & ng update @angular/cli@10 @angular/core@10 --allow-dirty --force & ng update @angular/core@11 @angular/cli@11 --allow-dirty --force & npx @angular/cli@12 update @angular/core@12 @angular/cli@12 --allow-dirty --force & npx @angular/cli@13 update @angular/core@13 @angular/cli@13 --allow-dirty --force"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/elements": "^13.3.11",
    "@angular/forms": "^13.3.11",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/router": "^13.3.11",
    "core-js": "^2.6.12",
    "document-register-element": "^1.14.10",
    "elements-zone-strategy": "^7.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular/cdk": "^7.1.0",
    "@angular/cli": "~13.3.8",
    "@angular/compiler-cli": "^13.3.11",
    "@angular/language-service": "^13.3.11",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-string-replace": "^1.1.2",
    "gulp-uglify": "^3.0.1",
    "http-server": "^0.11.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "ts-node": "~7.0.1",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}
