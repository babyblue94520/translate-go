{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "src/",
    "paths": {
      "@cui/core": [
        "ts/lib/cui/core"
      ],
      "@environment": [
        "environments/environment"
      ],
      "translate-go-test": [
        "dist/translate-go-test"
      ],
      "translate-go-test/*": [
        "dist/translate-go-test/*"
      ]
    },
    "plugins": [
      {
        "name": "@angular/language-service"
      },
      {
        "name": "tslint-language-service"
      }
    ],
    "useDefineForClassFields": false
  },
  "defaultProject": "translate-go"
}
